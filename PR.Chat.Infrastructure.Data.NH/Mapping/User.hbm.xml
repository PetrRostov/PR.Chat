<?xml version="1.0"?>
<hibernate-mapping 
   xmlns="urn:nhibernate-mapping-2.2" 
   assembly="PR.Chat.Domain"  
   namespace="PR.Chat.Domain" 
   default-access="nosetter.camelcase-underscore"
>

   <class name="User" table="Users" schema="Portal">
      <id name="Id" column="Id" type="Guid" >
         <generator class="guid.comb"/>
      </id>
      <property name="IsRegistered" column="IsRegistered" type="Boolean" not-null="true" update="false"/>
      <set name="Nicks" fetch="join" inverse ="true"  cascade="all" lazy="false">
         <key column="UserId" not-null="true" />
         <one-to-many class="Nick" not-found="ignore" />
      </set>
      
   </class>
   
</hibernate-mapping>