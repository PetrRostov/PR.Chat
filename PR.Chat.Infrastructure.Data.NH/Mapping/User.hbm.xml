<?xml version="1.0"?>
<hibernate-mapping 
   xmlns="urn:nhibernate-mapping-2.2" 
   assembly="PR.Chat.Domain"  
   namespace="PR.Chat.Domain" 
   default-access="nosetter.camelcase-underscore"
>

   <class name="User" table="Users" schema="Portal">
      <id name="Id" column="Id" type="Guid" >
         <generator class="assigned"/>
      </id>
      <property name="Name" column="Name" type="String" not-null="true" unique="true" />
      <property name="_password" column="Password" type="String" not-null="true" access="field"/>
      <property name="IsRegistered" column="IsRegistered" type="Boolean" not-null="true" />
   </class>
   
</hibernate-mapping>