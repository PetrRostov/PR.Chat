<?xml version="1.0"?>
<hibernate-mapping 
   xmlns="urn:nhibernate-mapping-2.2" 
   assembly="PR.Chat.Domain"  
   namespace="PR.Chat.Domain" 
   default-access="nosetter.camelcase-underscore"
>

   <class name="Nick" table="Nicks" schema="Portal">
      <id name="Id" column="Id" type="Guid" >
         <generator class="assigned"/>
      </id>
      <property name="Name" column="Name" type="String" not-null="true" unique="true" update="false"/>
      <many-to-one name="User" column="UserId" foreign-key="Nick_UserId_FK" class="User" not-null="true" />
      
   </class>
   
</hibernate-mapping>